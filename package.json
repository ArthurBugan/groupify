{
  "name": "youtube-channels-manager",
  "displayName": "Youtube channels manager",
  "version": "0.0.1",
  "description": "Manage youtube channels using folders and groups",
  "author": "arthurbugan",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@plasmohq/storage": "^1.5.0",
    "@radix-ui/react-accordion": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-select": "^1.2.1",
    "@radix-ui/react-slot": "^1.0.1",
    "@supabase/supabase-js": "^2.21.0",
    "@thedutchcoder/postcss-rem-to-px": "^0.0.2",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "cmdk": "^0.2.0",
    "hook": "link:@plasmohq/storage/hook",
    "lucide-react": "^0.206.0",
    "plasmo": "0.69.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.8.0",
    "react-virtuoso": "^4.3.6",
    "tailwind-merge": "^1.12.0",
    "tailwindcss-animate": "^1.0.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.227",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "prettier": "2.8.7",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "key": "$CRX_KEY",
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "*://*.youtube.com/*"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}
